{
  "name": "riverpodgen-freezed",
  "displayName": "riverpodgen-freezed",
  "description": "Inspired by Bloc extension, help us to create a provider + state using Riverpod gen + Freezed format.",
  "version": "2.0.0",
  "publisher": "trongnghia94",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "riverpod",
    "freezed"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "title": "Riverpod-Freezed",
    "commands": [
      {
        "command": "riverpodgen-freezed.new-provider",
        "title": "Riverpodgen-Freezed: New Provider"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "riverpodgen-freezed.new-provider",
          "group": "riverpodgenGroup@1",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/change-case": "^2.3.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.195",
    "@types/mkdirp": "^2.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1"
  }
}
